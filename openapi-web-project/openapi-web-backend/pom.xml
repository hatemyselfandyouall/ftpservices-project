<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>insigma.openapi.web</groupId>
    <artifactId>openapi-web-project</artifactId>
    <version>1.0.1-SNAPSHOT</version>
  </parent>
  <artifactId>openapi-web-backend</artifactId>
  <name>openapi-web-backend</name>
  
  <dependencies>
		<dependency>
			<groupId>org.javassist</groupId>
			<artifactId>javassist</artifactId>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi</artifactId>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
		</dependency>
		<dependency>
			<groupId>insigma.common</groupId>
			<artifactId>insigma-common-cache</artifactId>
		</dependency>
		<dependency>
			<groupId>insigma.common</groupId>
			<artifactId>insigma-common-util</artifactId>
		</dependency>
		<dependency>
			<groupId>insigma.common</groupId>
			<artifactId>insigma-common-framework-web</artifactId>
		</dependency>
	  <dependency>
		  <groupId>insigma.openapi.libs.sign</groupId>
		  <artifactId>openapi-lib-sign</artifactId>
	  </dependency>
		 <!-- insigma-lib-bizbase -->
		<dependency>
			<groupId>insigma.common.lib.bizbase</groupId>
 			<artifactId>insigma-lib-bizbase</artifactId>
		</dependency>
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>fastjson</artifactId>
		</dependency>
	    
		<dependency>
			<groupId>org.apache.httpcomponents</groupId>
			<artifactId>httpmime</artifactId>
		</dependency>
		
		<dependency>
			<groupId>insigma.empparam.facade.info</groupId>
 				<artifactId>empparam-facade-info</artifactId>
		</dependency>
		
		
		
	   
	   <dependency>
			<groupId>insigma.openapi.facade.info</groupId>
  			<artifactId>openapi-facade-info</artifactId>
  			<version>1.0.1-SNAPSHOT</version>
		</dependency>
		
		<dependency>
			<groupId>insigma.common.facade.collection</groupId>
			<artifactId>empcollection-facade-collection</artifactId>
		</dependency>
		<dependency>
			<groupId>insigma.archive.facade.info</groupId>
			  <artifactId>archive-facade-info</artifactId>
		</dependency>
	  <dependency>
		  <groupId>insigma.ftp.facade.info</groupId>
		  <artifactId>ftp-facade-info</artifactId>
	  </dependency>

	  <dependency>
		  <groupId>insigma.clouddata.facade.info</groupId>
		  <artifactId>clouddata-facade-info</artifactId>
	  </dependency>
    </dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.eclipse.jetty</groupId>
				<artifactId>jetty-maven-plugin</artifactId>
				<configuration>
					<httpConnector>
						<port>10500</port>
					</httpConnector>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>publish</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-assembly-plugin</artifactId>
						<configuration>
							<descriptors>
								<descriptor>assembly.xml</descriptor>
							</descriptors>
						</configuration>
						<executions>
							<execution>
								<id>make-assembly</id>
								<phase>package</phase>
								<goals>
									<goal>single</goal>
								</goals>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
	
	
</project>